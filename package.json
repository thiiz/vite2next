{
    "name": "vite2next",
    "version": "1.0.4",
    "description": "Automated tool to migrate Vite projects to Next.js",
    "main": "./src/cli.js",
    "type": "module",
    "bin": {
        "vite2next": "./src/cli.js"
    },
    "scripts": {
        "start": "node ./src/cli.js",
        "dev": "node ./src/cli.js"
    },
    "exports": {
        ".": "./src/cli.js",
        "./api": "./src/cli.js"
    },
    "keywords": [
        "vite",
        "next.js",
        "migration",
        "cli",
        "react"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "chalk": "^5.3.0",
        "commander": "^11.1.0",
        "fs-extra": "^11.1.1",
        "glob": "^10.3.10",
        "inquirer": "^9.2.12",
        "jsdom": "^22.1.0",
        "ora": "^7.0.1",
        "postcss": "^8.4.31",
        "postcss-js": "^4.0.1"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/thiiz/vite2next"
    },
    "bugs": {
        "url": "https://github.com/thiiz/vite2next/issues"
    },
    "homepage": "https://github.com/thiiz/vite2next#readme"
